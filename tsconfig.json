{
    "compilerOptions": {
        "strict": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "esModuleInterop": true,
        "moduleResolution": "node",
        "outDir": "dist",
        "skipLibCheck": true,
    },
    "exclude": ["node_modules", "client", "build"]
  }